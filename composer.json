{
    "name": "jaywood/simple-discord-sso",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "JayWood",
            "email": "jjwood2004@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "com\\plugish\\discord\\sso\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/phpunit/TestCase.php"
        ]
    },
    "scripts": {
        "test": "pest",
        "lint": "phpcs .",
        "fix": "phpcbf"
    },
    "require-dev": {
        "brain/monkey": "2.*",
        "pestphp/pest": "^v2.18.1",
        "wp-coding-standards/wpcs": "^3.0.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^v1.0.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
