{
    "name": "jaywood/simple-discord-sso",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "JayWood",
            "email": "jjwood2004@gmail.com"
        }
    ],
    "require": {
      "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "com\\plugish\\discord\\sso\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/phpunit/TestCase.php"
        ]
    },
    "scripts": {
        "test": "pest",
        "lint": "phpcs .",
        "fix": "phpcbf"
    },
    "require-dev": {
        "brain/monkey": "2.*",
        "pestphp/pest": "^1.21",
        "wp-coding-standards/wpcs": "^2.3",
        "phpcompatibility/phpcompatibility-wp": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "^v0.7"
    }
}
